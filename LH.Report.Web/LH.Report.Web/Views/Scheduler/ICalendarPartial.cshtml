@{
    SchedulerExtension scheduler = Html.DevExpress().Scheduler(SchedulerDemoHelper.ExportSchedulerSettings);
    if (TempData["SchedulerErrorText"] != null) {
        scheduler.SetErrorText((string)TempData["SchedulerErrorText"]);
    }
}
@scheduler.Bind(Model, CarsDataProvider.GetCars()).GetHtml()